{
  "name": "go-ai-backend",
  "version": "1.0.0",
  "description": "Node JS application serving as the backend of a GO playing program",
  "main": "index.js",
  "scripts": {
    "forever": "node run_forever.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axyl-casc/GO-AI-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axyl-casc/GO-AI-backend/issues"
  },
  "homepage": "https://github.com/axyl-casc/GO-AI-backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "forever-monitor": "^3.0.3",
    "jsdom": "^25.0.1",
    "shell-quote": "^1.8.2",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.25.11",
    "tenuki": "^0.3.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "electron": "^34.2.0"
  },
  "build": {
    "productName": "serve",
    "win": {
      "target": "portable"
    },
    "files": [
      "**/*",
      "!**/*.map",
      "!gtp_logs",
      "!Launcher.spec",
      "!node_modules/**/test",
      "!node_modules/**/example",
      "!node_modules/**/benchmark",
      "!node_modules/**/.bin",
      "!*.py",
      "!*.jsonc",
      "!dist",
      "!readme.md",
      "!notes.txt",
      "!*.db",
      "!baduk_AI/**",
      "!baduk_AI"
    ],
    "asarUnpack": [
      "app.js"
    ]
  }
}
